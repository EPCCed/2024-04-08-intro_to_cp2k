&GLOBAL
  PROJECT S20Acetone
  RUN_TYPE GEO_OPT
  PREFERRED_DIAG_LIBRARY SL
  PRINT_LEVEL medium
&END GLOBAL
 &MOTION
 &GEO_OPT
 MAX_ITER 200
 &END GEO_OPT
 &PRINT
  &STRUCTURE_DATA
   DISTANCE 3 4
   DISTANCE 1 3
   ANGLE 1 3 2
   DIHEDRAL_ANGLE 4 3 1 2
  &END STRUCTURE_DATA
  &END PRINT
 &END MOTION
&FORCE_EVAL
  METHOD Quickstep
&PROPERTIES
  &TDDFPT
   KERNEL sTDA
   &sTDA
    FRACTION 0.25
   &END sTDA
   NSTATES 1
   MAX_ITER   100
   CONVERGENCE [eV] 1.0e-7
   RKS_TRIPLETS F
  &END TDDFPT
&END PROPERTIES
  &DFT
    &QS
      METHOD GPW
      EPS_DEFAULT 1.0E-17
      EPS_PGF_ORB 1.0E-20
    &END QS
 &EXCITED_STATES T
  STATE 1
 &END EXCITED_STATES
    &SCF
      SCF_GUESS restart
      &OT
         PRECONDITIONER FULL_ALL
         MINIMIZER DIIS
      &END OT
      &OUTER_SCF
         MAX_SCF 900
         EPS_SCF 1.0E-7
      &END OUTER_SCF
      MAX_SCF 10
      EPS_SCF 1.0E-7
    &END SCF
POTENTIAL_FILE_NAME POTENTIAL_UZH
BASIS_SET_FILE_NAME BASIS_MOLOPT_UZH
BASIS_SET_FILE_NAME BASIS_ccGRB_UZH
BASIS_SET_FILE_NAME BASIS_ADMM_UZH_mod
    &MGRID
      CUTOFF 800
      REL_CUTOFF 80
    &END MGRID
    &POISSON
       PERIODIC NONE
       POISSON_SOLVER WAVELET 
    &END
    &XC
     &XC_FUNCTIONAL PBE0
     &END XC_FUNCTIONAL
    &HF
        &MEMORY
          MAX_MEMORY 9000
          EPS_STORAGE_SCALING 0.1
        &END
      &END
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC [angstrom] 14.0 14.0 14.0
      PERIODIC NONE
    &END CELL
    &COORD
 C -0.191434 -0.562180 1.301879
 C -0.191434 -0.562180 -1.301879
 C -0.191434 0.194440 0.000000
 O 0.573795 1.262750 0.000000
 H -0.404809 0.102891 2.134137
 H 0.784281 -1.027604 1.477309
 H -0.951748 -1.336527 1.259750
 H -0.404809 0.102891 -2.134137
 H -0.951748 -1.336527 -1.259750
 H 0.784281 -1.027604 -1.477309
    &END COORD
    &TOPOLOGY
     &CENTER_COORDINATES T
     &END
    &END
    &KIND H 
      BASIS_SET ORB TZVP-MOLOPT-PBE0-GTH-q1
      POTENTIAL GTH-PBE0-q1
    &END KIND
    &KIND O 
      BASIS_SET ORB TZVP-MOLOPT-PBE0-GTH-q6
      POTENTIAL GTH-PBE0-q6
    &END KIND
    &KIND C 
      BASIS_SET ORB TZVP-MOLOPT-PBE0-GTH-q4
      POTENTIAL GTH-PBE0-q4
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
