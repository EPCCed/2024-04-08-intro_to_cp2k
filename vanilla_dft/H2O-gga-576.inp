@SET NREPX 1

&FORCE_EVAL
  METHOD QS
  &DFT
    BASIS_SET_FILE_NAME HFX_BASIS
    POTENTIAL_FILE_NAME POTENTIAL
    &MGRID
      CUTOFF 500
      REL_CUTOFF 60
    &END MGRID
    &QS
      EPS_DEFAULT 1.0E-12
      WF_INTERPOLATION PS
      EXTRAPOLATION_ORDER 3
    &END QS
    &SCF
      SCF_GUESS ATOMIC
      &OT ON
        PRECONDITIONER FULL_SINGLE_INVERSE
        MINIMIZER DIIS
      &END OT

      &PRINT
        &RESTART ON
        &END
      &END
      EPS_SCF      1.0E-7
    &END SCF
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 9.85280000E+00  1.97056000E+01  2.95584000E+01
    &END CELL
    &TOPOLOGY
      COORD_FILE_FORMAT XYZ
      COORD_FILE_NAME H2O-576.xyz
    &END TOPOLOGY
    &KIND H
      BASIS_SET TZV2P-GTH
      POTENTIAL GTH-PADE-q1
    &END KIND
    &KIND O
      BASIS_SET TZV2P-GTH
      POTENTIAL GTH-PADE-q6
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT H2O-64
  RUN_TYPE MD
  PRINT_LEVEL LOW
  PREFERRED_DIAG_LIBRARY SCALAPACK
&END GLOBAL
&MOTION
  &MD
    ENSEMBLE NVE
    STEPS 10
    TIMESTEP 0.5
    TEMPERATURE 300.0
    &PRINT
      &ENERGY OFF
      &END
    &END
  &END MD
  &PRINT
    &RESTART_HISTORY OFF
    &END
    &RESTART OFF
    &END
    &TRAJECTORY OFF
    &END
  &END
&END MOTION
